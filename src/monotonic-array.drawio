        <mxCell id="lmmgZAD-LvhzfWVlW3RH-11" value="&lt;h1&gt;Time Complexity&lt;/h1&gt;&lt;p&gt;O(Nlog(N) + M(log(M))&lt;/p&gt;&lt;p&gt;This is due to the sorting at the beginning&lt;/p&gt;&lt;p&gt;N = number of elements in array one&lt;/p&gt;&lt;p&gt;M = number of elements in array two&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="70" y="620" width="320" height="150" as="geometry" />
        </mxCell>
        <mxCell id="lmmgZAD-LvhzfWVlW3RH-12" value="&lt;h1&gt;Space Complexity&lt;/h1&gt;&lt;p&gt;O(1)&lt;/p&gt;&lt;p&gt;We only ever keep two elements in the resultant array&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
